<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Custome" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:textColorSecondary">#FFFFFF</item>

    </style>
    <style name="FirstFragment" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorPrimary">#f40606</item>
        <item name="colorPrimaryDark">#690c04</item>
        <item name="colorAccent">@color/whiteColor</item>
        <item name="android:textColorPrimary">#16d492</item>
        <item name="android:textColorSecondary">@color/colorAccent</item>

    </style>

    <style name="AppDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="CustomButton">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:typeface">normal</item>
        <item name="android:textSize" >9pt</item>
        <item name="android:paddingRight">7dp</item>
        <item name="android:paddingLeft">7dp</item>
    </style>

</resources>
